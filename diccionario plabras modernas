meme_dict = {
            "CRINGE": "Algo excepcionalmente raro o embarazoso",
            "LOL": "Una respuesta común a algo gracioso",
            "ROFL" : "una respuesta a una broma" ,
            "SHEESH":" ligera desaprobación",
            "VIVES":"lo que se siente no lo que se piensa la vibras buenas o malas que te de la persona",
            "AGGRO" : "ponerse agresivo/enojado"
            }
word = input("Escribe una palabra que no entiendas (¡con mayúsculas!): ").upper()
if word in meme_dict.keys():
    resultado= meme_dict[word]
    print("significa",resultado)
else:
    print("no se encontro")
